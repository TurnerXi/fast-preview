{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "allowJs": true,
    "skipLibCheck": true,
    "declaration": true,
    "outDir": "./lib",
  },
  "include": [
    "./src"
  ],
  "exclude": [
    "node_modules"
  ],
}